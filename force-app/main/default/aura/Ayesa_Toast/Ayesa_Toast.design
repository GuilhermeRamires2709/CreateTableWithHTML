<design:component label="Toast">
    <design:attribute name="text" label="Title" description="Text of the title you want to display"/>
    <design:attribute name="textBody" label="Text" description="Text of the body you want to display"/>
    <design:attribute name="textAlign" label="Align" description="Alignment text of the body" datasource="Left,Right,Center"/>
